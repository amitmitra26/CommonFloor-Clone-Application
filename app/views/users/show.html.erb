<% provide(:title, @user.name) %>
<div class="row">
<aside>
  <section class="user_info">
    <h1>

      <% if @auth %>
Signed in as: <%= current_user.name %> - <%= image_tag(@auth['info']['image'], width: '50', height: '50', class: 'image-circle')%>
   <% end %>

    <p> <%= @user.name %></p>

    </h1>


    <ul>
      <% pnm = Property.find(@properties_visited) %>
      <% pnm.each do |p| %>
      <li ><%= link_to p.title, p, style: "color:blue;" %></li>
      <% end %>
    </ul>


  </section>
  <div>
</aside>
</div>
