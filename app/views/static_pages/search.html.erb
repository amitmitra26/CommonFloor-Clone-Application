<h1>Property Search:</h1>
<% unless @search_properties_for.empty? %>
  <div>
    <ul>
      <% @search_properties_for.each do |pr| %>
        <li><%= link_to pr.title, property_path(pr) %></li>
      <% end %>
    </ul>
  </div>
<% else %>
  <p class="text-center">No Property Available as per your criteria</p>
<% end %>
<%= link_to "Back to Home", root_path %>
